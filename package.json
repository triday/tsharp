  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha --reporter mochawesome",
    "cover": "nyc npm test",
    "check": "nyc --check-coverage --lines 80 --functions 80 --branches 80 npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/triday/tsharp.git"
  },
  "author": "obpt123",
